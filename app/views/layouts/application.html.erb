</script>
</head>
<body>
  <div class="page-header">
    <h1>CountMe</h1>

  <div class = "btn btn-danger">
   <%= link_to 'Sign Out', destroy_user_session_path, method: 'DELETE', :class => "btn-danger" %>
   </div>
     </div>
<%= yield %>

</body>
</html>
